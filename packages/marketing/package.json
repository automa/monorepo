    "node": ">=22.3.0"
  },
  "browserslist": {
    "production": [
      "since 2010",
      ">0.3%",
      "not dead",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "since 2010",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mdx-js/loader": "~3.1.0",
    "@next/mdx": "~14.2.3",
