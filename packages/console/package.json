{
  "name": "@automa/console",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@apollo/client": "~3.7.5",
    "@automa/common": "link:../common",
    "@qatalog/react-flex": "~0.2.0",
    "@radix-ui/react-toast": "~1.1.1",
    "@radix-ui/react-tooltip": "~1.0.2",
    "@reduxjs/toolkit": "~1.7.0",
    "@tanstack/react-table": "~8.5.22",
    "axios": "~1.1.2",
    "csstype": "~3.0.10",
    "fontfaceobserver": "~2.3.0",
    "logdown": "~3.3.1",
    "polished": "~4.1.4",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-redux": "~7.2.6",
    "react-responsive": "~9.0.2",
    "react-router-dom": "~6.0.2",
    "reset-css": "~5.0.1",
    "styled-components": "~5.3.3"
  },
  "scripts": {
    "build": "tsc && vite build",
    "build-analyze": "BUILD_SOURCEMAP=1 vite build && source-map-explorer 'build/assets/*.js'",
    "build-stats": "BUILD_STATS=1 vite build",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "start": "vite",
    "storybook": "storybook dev -p 6006",
    "storybook-build": "storybook build",
    "test": "vitest run",
    "test-cover": "vitest run --coverage",
    "test-watch": "vitest watch",
    "test-storybook": "test-storybook",
    "test-storybook-cover": "test-storybook --coverage",
    "update-boilerplate": "git pull --no-rebase -s subtree -X subtree=packages/console https://git.sr.ht/~pksunkara/frontend-react master"
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "browserslist": {
    "production": [
      "since 2010",
      ">0.3%",
      "not dead",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "since 2010",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-coverage": "~0.0.8",
    "@storybook/addon-essentials": "~7.0.18",
    "@storybook/addon-interactions": "~7.0.18",
    "@storybook/addon-links": "~7.0.18",
    "@storybook/jest": "~0.1.0",
    "@storybook/node-logger": "~7.0.18",
    "@storybook/react-vite": "~7.0.18",
    "@storybook/test-runner": "~0.10.0",
    "@storybook/testing-library": "~0.1.0",
    "@testing-library/jest-dom": "~5.16.2",
    "@testing-library/react": "~14.0.0",
    "@testing-library/user-event": "~14.4.3",
    "@types/fontfaceobserver": "~2.1.0",
    "@types/node": "~18.16.19",
    "@types/react": "~18.0.34",
    "@types/react-dom": "~18.0.11",
    "@types/react-router-dom": "~5.3.2",
    "@types/styled-components": "~5.1.18",
    "@typescript-eslint/eslint-plugin": "~5.59.7",
    "@typescript-eslint/parser": "~5.59.7",
    "@vitejs/plugin-react": "~4.0.0",
    "@vitest/coverage-c8": "~0.31.1",
    "eslint": "~8.9.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-import-resolver-typescript": "~3.5.5",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-jsx-a11y": "~6.7.1",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-react": "~7.32.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-storybook": "~0.6.12",
    "jsdom": "~22.1.0",
    "prettier": "~2.5.1",
    "rollup-plugin-webpack-stats": "~0.2.0",
    "source-map-explorer": "~2.5.2",
    "storybook": "~7.0.18",
    "typescript": "~4.4.4",
    "vite": "~4.3.2",
    "vite-plugin-babel-macros": "~1.0.6",
    "vite-plugin-checker": "~0.6.0",
    "vite-plugin-eslint": "~1.8.1",
    "vite-plugin-svgr": "~3.2.0",
    "vite-tsconfig-paths": "~4.2.0",
    "vitest": "~0.31.1",
    "vitest-fetch-mock": "~0.2.2"
  }
}
